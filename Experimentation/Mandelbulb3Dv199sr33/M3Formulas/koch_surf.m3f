	// Fold
	z.xy = polyfoldsym(z.xy,shiftx,shifty,angleshift);
	z.y=YOff-abs(z.y-YOff);
	z.x+=1./3.;if(z.z>z.x) z.xz=z.zx; z.x-=1./3.;
	// variation is
	// z.x+=1./3.;if(z.z>z.x) z.xz=z.zx; z.x-=1./3.;
	z.x-=1./3.;if(z.z>z.x) z.xz=z.zx; z.x+=1./3.; // normal formula
	z = Scale * (z + Offset);
	z = rot * z;
	z = z - Offset;
